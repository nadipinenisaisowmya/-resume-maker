<body>

    <style>
        #full-box {
            border-width: 1.5px;
            border-style: solid;
            border-color: black;
            {{!-- background-color: rgb(50, 109, 138); --}}
        }
        #header
        {
            {{!-- background-color:rgb(15, 76, 87); --}}
        }
        #name {
            color: black;
            font-family: 'Times New Roman', Times, serif;
            font-size: 2rem;
        }
        #personal_details {
            color: black;
        }
        #date
        {
                color:rgb(15, 76, 87);
        }
        #ed {
            font-size: 1.2rem;
            font-family: Cambria, Cochin, Georgia, Times, 'Times New Roman', serif
        }
        #work {
            font-size: 1.2rem;
            font-family: Cambria, Cochin, Georgia, Times, 'Times New Roman', serif
        }
        #skills_section {
            font-size: 1.2rem;
            font-family: Cambria, Cochin, Georgia, Times, 'Times New Roman', serif
        }
        #pro
        {
            font-size: 1.2rem;
            font-family: Cambria, Cochin, Georgia, Times, 'Times New Roman', serif
        }
        #hobby
        {
                font-size: 1.2rem;
            font-family: Cambria, Cochin, Georgia, Times, 'Times New Roman', serif
        }
        
    </style>

    <header class="p-3 bg-dark text-white">
        <div class="container-fluid">
            <div class="d-flex flex-wrap align-items-center justify-content-center justify-content-lg-start">
                <a href="/" class="d-flex align-items-center mb-2 mb-lg-0 text-white text-decoration-none">
                    <img width="100px" height="50px"
                        src="https://encrypted-tbn0.gstatic.com/images?q=tbn:ANd9GcSLYod7KZAkri6S212bhV4Apg1kX1s1SnaNVg&usqp=CAU">
                </a>

                <ul class="nav col-12 col-lg-auto me-lg-auto mb-2 justify-content-center mb-md-0">
                    <li><a href="#" class="nav-link px-2 text-white">Home</a></li>
                    <li><a href="#" class="nav-link px-2 text-white">Features</a></li>
                    <li><a href="#" class="nav-link px-2 text-white">Pricing</a></li>
                    <li><a href="#" class="nav-link px-2 text-white">FAQs</a></li>
                    <li><a href="#" class="nav-link px-2 text-white">About</a></li>
                </ul>

                <form class="col-12 col-lg-auto mb-3 mb-lg-0 me-lg-3">
                    <input type="search" class="form-control form-control-dark" placeholder="Search..."
                        aria-label="Search">
                </form>

                <div class="text-end">
                    <button type="button" class="btn btn-outline-light me-2">Login</button>
                    <button type="button" class="btn btn-warning">Sign-up</button>
                </div>
            </div>
        </div>
    </header>



    <div class="container" id="full-box">



        <div class="container" id="header">

            <div class="row" id="name">
                <div class="col">
                    <b>
                        <p>{{first_name }} {{second_name}}</p>
                    </b>
                </div>
            </div>

            <div class="row" id="personal_details">
                <div class="col">
                    <p> <svg xmlns="http://www.w3.org/2000/svg" width="16" height="16" fill="currentColor"
                            class="bi bi-envelope-fill" viewBox="0 0 16 16">
                            <path
                                d="M.05 3.555A2 2 0 0 1 2 2h12a2 2 0 0 1 1.95 1.555L8 8.414.05 3.555ZM0 4.697v7.104l5.803-3.558L0 4.697ZM6.761 8.83l-6.57 4.027A2 2 0 0 0 2 14h12a2 2 0 0 0 1.808-1.144l-6.57-4.027L8 9.586l-1.239-.757Zm3.436-.586L16 11.801V4.697l-5.803 3.546Z" />
                        </svg> {{em}} <svg xmlns="http://www.w3.org/2000/svg" width="16" height="16" fill="currentColor"
                            class="bi bi-telephone-fill" viewBox="0 0 16 16">
                            <path fill-rule="evenodd"
                                d="M1.885.511a1.745 1.745 0 0 1 2.61.163L6.29 2.98c.329.423.445.974.315 1.494l-.547 2.19a.678.678 0 0 0 .178.643l2.457 2.457a.678.678 0 0 0 .644.178l2.189-.547a1.745 1.745 0 0 1 1.494.315l2.306 1.794c.829.645.905 1.87.163 2.611l-1.034 1.034c-.74.74-1.846 1.065-2.877.702a18.634 18.634 0 0 1-7.01-4.42 18.634 18.634 0 0 1-4.42-7.009c-.362-1.03-.037-2.137.703-2.877L1.885.511z" />
                        </svg> {{ph}} <svg xmlns="http://www.w3.org/2000/svg" width="16" height="16" fill="currentColor"
                            class="bi bi-geo-alt-fill" viewBox="0 0 16 16">
                            <path
                                d="M8 16s6-5.686 6-10A6 6 0 0 0 2 6c0 4.314 6 10 6 10zm0-7a3 3 0 1 1 0-6 3 3 0 0 1 0 6z" />
                        </svg> {{co}} </p>
                </div>

            </div>

        </div>

        <hr>

        <div class="container" id="summary">
            <div class="row">
                <div class="col">
                    <p id="pro"> <b> <u> PROFESSIONAL SUMMARY</u></b></p>
                </div>
            </div>
            <div class="row">
                <div class="col">
                    <p>{{pro}}</p>
                </div>
            </div>

        </div>
        <div class="container" id="Education">
            <div class="row">
                <div class="col" id="ed">
                    <p> <b> <u> EDUCATION</u></b></p>
                </div>
            </div>
            <div class="row">
                <div class="col">
                    <p> {{deg}} from {{col}} </p>
                </div>
            </div>



        </div>


        <div class="container" id="work-experience">
            <div class="row">
                <div class="col" id="work">
                    <p> <b> <u> WORK EXPERIENCE</u></b></p>
                </div>
            </div>

            <div class="row" id="job">
                <div class="col">
                    <p><b><u> {{jo}}</u> , {{emp}} ,{{ct}}   </b></p>
                    <p id="date"><b>( {{sd}} to {{ld}} ) </b> </p>
                    <p>{{des}}</p>
                </div>
            </div>

            
                    <p><b> </b></p>
                
            
                    
                
        </div>






        <div class="container" id="skills_section">

            <div class="row">
                <div class="col">
                    <p><b><u>SKILLS SECTION </u></b> </p>
                </div>
            </div>
            <div class="row">
                <div class="col" id="skills">
                    <p>{{sk1}} {{lvl1}}  {{sk2}} {{lvl2}}
                    </p>
                </div>
                <div class="col">
                    {{!-- <p> {{sk2}} {{lvl2}}
                    </p> --}}
                </div>
                <div class="col">
                    <p> {{sk3}} {{lvl3}} {{sk4}} {{lvl4}}
                    </p>
                </div>
                <div class="col">
                    {{!-- <p> {{sk4}} {{lvl4}} --}}
                    {{!-- </p> --}}
                </div>
            </div>

            <div class="row">
                <div class="col">
                    <p> {{sk5}} {{lvl5}} {{sk6}} {{lvl6}}
                    </p>
                </div>
                <div class="col">
                    {{!-- <p> {{sk6}} {{lvl6}} --}}
                    {{!-- </p> --}}
                </div>
            </div>
        </div>



 <div class="container" id="hobby">
            <div class="row">
                <div class="col" id="work">
                    <p> <b> <u> HOBBIES</u></b></p>
                </div>
            </div>

            <div class="row" id="job">
                <div class="col">
                    <p>{{ho}} </p>
                </div>
            </div>

            
                    
                
            
                    
                
        </div>
       








